<!DOCTYPE html>
<html>
<head>
  <title>Information Management System</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="container" class="container_12">

  <div id="header" class="grid_12">
    <div id="h_l" class="grid_6 alpha">
      <p>Information Management System</p>
    </div>

    <div id="h_2" class="grid_6 omega">
      <%= render 'shared/layouts/account' %>
    </div>
  </div>

  <div id="subheader" class="grid_12">
    <div id="sh_1" class="grid_6 alpha">
        <div>
          <%= render 'shared/layouts/subheader' %>
        </div>
    </div>
    <div id="sh_2" class="grid_6 omega">
    </div>
  </div>
  <div id="" class="grid_12 ">
    <div class="c_boxes">
      <%= yield %>
    </div>
  </div>
  <div id="" class="grid_12 d_boxes">
    <div class="grid_4 alpha d_box"><div><h6>Ticket Monitoring</h6></div></div>
    <div class="grid_4 d_box"><div><h6>User Activity</h6></div></div>
    <div class="grid_4 omega d_box"><div><h6>Notices</h6></div></div>
  </div>

  <div id="" class="grid_12 d_boxes">
    <div class="grid_4 alpha d_box"><div><h6>Application Information</h6></div></div>
    <div class="grid_4 d_box"><div><h6>Graph</h6></div></div>
    <div class="grid_4 omega d_box"><div<h6>Extras</h6></div></div>
  </div>

</div>
<div class="clearfix"></div>
<%= render 'shared/layouts/footer' %>

</body>
</html>